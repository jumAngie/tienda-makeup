@model Maquillaje.WebUI.Models.CategoriaViewModel

@{
    ViewData["Title"] = "Detalles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="text-center">
    <img src="~/Content/iMG/giphy.gif" width="100" />
    <h1 style="color:black;font-family:'Segoe Print'">Detalles Categoria</h1>
</div>
<div>
    <hr />



    <center>
        <dl class="dl-horizontal">

            <div class="row">
                <div class="col-sm-2"></div>

                <div class="col-sm-4">
            <dt>
                Categoría Id
            </dt>

            <dd class="form-control">
                @Html.DisplayFor(model => model.cat_Id)
            </dd>
</div>


<div class="col-sm-4">
    <dt>
        Categoría Descripcion
    </dt>

    <dd class="form-control">
        @Html.DisplayFor(model => model.cat_Descripcion)
    </dd>
</div>
    </div>



<br />
<br />

<div class="row">
    <div class="col-sm-2"></div>

    <div class="col-sm-4">
        <dt>
            Usuario Creación
        </dt>

        <dd class="form-control">
            @Html.DisplayFor(model => model.cat_UsuCrea)
        </dd>

    </div>


    <div class="col-sm-4">
        <dt>
            Usuario Modificación
        </dt>

        <dd class="form-control">
            @Html.DisplayFor(model => model.cat_UsuModi)
        </dd>

    </div>
</div>

<br />
<br />

<div class="row">
    <div class="col-sm-2"></div>

    <div class="col-sm-4">
        <dt>
            Fecha Creación
        </dt>

        <dd class="form-control">
            @Html.DisplayFor(model => model.cat_FechaCrea)
        </dd>

    </div>


    <div class="col-sm-4">
        <dt>
            Fecha Modificación
        </dt>

        <dd class="form-control">
            @Html.DisplayFor(model => model.cat_FechaModi)
        </dd>

    </div>
</div>

    </dl>
    </center>




<div>
    <a class="btn btn-light" asp-action="Index">Volver</a>
</div>
<center><strong><h2>@ViewBag.NombreCategoria</h2></strong></center>
