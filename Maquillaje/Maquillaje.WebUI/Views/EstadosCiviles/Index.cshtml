@model IEnumerable<Maquillaje.WebUI.Models.EstadosCivilesViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.est_ID)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.est_Descripcion)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.est_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.est_Descripcion)
            </td>


            <td>
                <a asp-action="Edit" asp-route-id="@item.est_ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.est_ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.est_ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
